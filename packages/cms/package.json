{
  "name": "safis-cms",
  "version": "0.0.1",
  "description": "A headless, git-based and GraphQL native CMS",
  "scripts": {
    "build": "babel ./ -d dist --ignore node_modules",
    "prebuild": "npm run clean",
    "clean": "rm -rf dist/*",
    "dev": "npm-run-all --print-label --parallel dev:**",
    "dev:client": "echo \"client is not implemented yet\"",
    "dev:server": "babel-node server.js",
    "start": "npm-run-all --parallel start:**",
    "prestart": "npm run build",
    "start:server": "NODE_ENV=production PORT=5000 node ./dist/server.js",
    "start:client": "echo \"client is not implemented yet\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/safis-io/safis-cms.git"
  },
  "keywords": [
    "safis-cms",
    "cms",
    "git based cms",
    "content management system"
  ],
  "author": {
    "name": "Safis",
    "email": "team@safis.io"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/safis-io/safis-cms/issues"
  },
  "homepage": "https://github.com/safis-io/safis-cms#readme",
  "engines": {
    "node": ">=14.17.0",
    "npm": ">=6.14.13"
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "lodash.camelcase": "^4.3.0",
    "lodash.get": "^4.4.2",
    "lodash.startcase": "^4.4.0",
    "regenerator-runtime": "^0.13.5",
    "safis-cms-server": "beta"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "npm-run-all": "^4.1.5"
  }
}
